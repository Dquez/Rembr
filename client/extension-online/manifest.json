{
  "key": "",
  "manifest_version": 2,
  "short_name": "Rembr App",
  "name": "React Extension",
  "version": "1.0.1", 
  "permissions": ["http://*/", "https://*/", "activeTab", "tabs", "declarativeWebRequest", "bookmarks", "identity", "identity.email", "http://localhost:3000/*", "http://localhost:3001/*", "https://accounts.google.com/*", "https://www.googleapis.com/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>", "http://localhost:3001/*"],
      "js": [ "jquery.js", "content-script.js"]
    }
  ],
  "oauth2": {
    "client_id": "610105421590-cmlmi74luvqehuok9pbd8bfmeacf3qn5.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
      
    ]
  },
  "browser_action": {
    "default_popup": "rembr-ext.html",
    "default_title": "React Ext"
  }
}